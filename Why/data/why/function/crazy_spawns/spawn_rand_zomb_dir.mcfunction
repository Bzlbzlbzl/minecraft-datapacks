# Spawns markers based on the zomnbie's tag, and determines a random location
# Also spawns the zombies because no reason to make separate function
scoreboard players set %direction crazy_spawns 0

execute if entity @s[tag=crazy+X] at @s run summon marker ~ ~ ~ {Tags:["crazySpawned","crazy+X"]}
execute if entity @s[tag=crazy-X] at @s run summon marker ~ ~ ~ {Tags:["crazySpawned","crazy-X"]}
execute if entity @s[tag=crazy+Z] at @s run summon marker ~ ~ ~ {Tags:["crazySpawned","crazy+Z"]}
execute if entity @s[tag=crazy-Z] at @s run summon marker ~ ~ ~ {Tags:["crazySpawned","crazy-Z"]}

execute as @e[type=marker,tag=crazySpawned,limit=1,sort=random] run tag @s add crazyPicked

execute as @e[type=marker,limit=1,tag=crazyPicked] if entity @s[tag=crazy+X] run scoreboard players set %direction crazy_spawns 1
execute as @e[type=marker,limit=1,tag=crazyPicked] if entity @s[tag=crazy-X] run scoreboard players set %direction crazy_spawns 2
execute as @e[type=marker,limit=1,tag=crazyPicked] if entity @s[tag=crazy+Z] run scoreboard players set %direction crazy_spawns 3
execute as @e[type=marker,limit=1,tag=crazyPicked] if entity @s[tag=crazy-Z] run scoreboard players set %direction crazy_spawns 4

kill @e[type=marker,tag=crazySpawned]

# Now that direction is determined, summon the zombie
# Note fallback spawn is when %direction matches 0
